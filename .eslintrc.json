{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "prettier/prettier": "error",
    "class-methods-use-this": "off",
    "no-underscore-dangle": ["error", { "allow": ["enforceInMethodNames"] }]
  }
}
